{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": "off",
        "react/prop-types": "off",
        "no-unused-vars": "off",
        "react/jsx-props-no-spreading": "off",
        "react/no-unstable-nested-components": "off",
        "react/no-danger": "off"
    }
}
